    "warning": {
      "dark": "vscode_warning",
      "light": "vscode_warning"
    },
    "success": {
      "dark": "vscode_success",
      "light": "vscode_success"
    },
    "info": {
      "dark": "vscode_info",
      "light": "vscode_info"
    },
    "text": {
      "dark": "vscode_fg",
      "light": "#1E1E1E"
    },
    "textMuted": {
      "dark": "vscode_muted",
      "light": "#6A6A6A"
