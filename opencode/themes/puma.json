    },
    "syntaxFunction": {
      "dark": "vscode_function",
      "light": "vscode_function"
    },
    "syntaxVariable": {
      "dark": "vscode_variable",
      "light": "vscode_variable"
    },
    "syntaxString": {
      "dark": "vscode_string",
      "light": "vscode_string"
    },
    "syntaxNumber": {
      "dark": "vscode_number",
      "light": "vscode_number"
    },
    "syntaxType": {
      "dark": "vscode_type",
      "light": "vscode_type"
    },
    "syntaxOperator": {
      "dark": "vscode_keyword",
      "light": "vscode_keyword"
    },
    "syntaxPunctuation": {
      "dark": "vscode_fg",
      "light": "#1E1E1E"
    }
