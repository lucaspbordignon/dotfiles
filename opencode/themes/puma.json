{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "vscode_bg": "#1E1E1E",
    "vscode_fg": "#D4D4D4",
    "vscode_panel": "#252526",
    "vscode_border": "#3E3E42",
    "vscode_border_active": "#007FD4",
    "vscode_selection": "#264F78",
    "vscode_comment": "#6A9955",
